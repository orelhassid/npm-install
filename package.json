{
  "name": "npm-install",
  "displayName": "npm-install",
  "description": "Installing npm packages using GUI with just one click",
  "version": "0.0.1",
  "author": {
    "name": "Orel Hassid"
  },
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:npm-install.helloWorld",
    "onCommand:npm-install.time"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "npm-install.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "npm-install.time",
        "title": "What the time?"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
